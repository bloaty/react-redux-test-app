{
  "name": "react-redux-test-app",
  "version": "0.0.1",
  "description": "Fooling around",
  "main": "index.js",
  "scripts": {
    "build:dev": "NODE_ENV=development webpack --config ./webpack.config.js --mode=development",
    "build:prod": "NODE_ENV=production webpack --config ./webpack.config.js --mode=production",
    "lint:ts": "tslint --config tslint.json --format stylish --project tsconfig.json",
    "lint:styles": "sass-lint --config ./sass-lint.yml --syntax scss --no-exit --verbose",
    "serve:dev": "NODE_ENV=development webpack-dev-server --color --config ./webpack.config.js --mode=development",
    "serve:prod": "NODE_ENV=production webpack-dev-server --color --config ./webpack.config.js --mode=production",
    "start:dev": "npm-run-all serve:dev",
    "start:prod": "npm-run-all serve:prod",
    "test": "echo 'Error: no tests!' && exit 1"
  },
  "author": "bloaty",
  "license": "MIT",
  "sasslintConfig": "./sass-lint.yml",
  "husky": {
    "hooks": {
      "commit-msg": "npm-run-all lint:styles lint:ts"
    }
  },
  "dependencies": {
    "react": "16.7.x",
    "react-dom": "16.7.x",
    "react-redux": "6.0.x",
    "redux": "4.0.x"
  },
  "devDependencies": {
    "@types/react": "16.7.x",
    "@types/react-dom": "16.0.x",
    "@types/react-redux": "6.0.x",
    "awesome-typescript-loader": "5.2.x",
    "clean-webpack-plugin": "1.0.x",
    "css-loader": "2.1.x",
    "file-loader": "3.0.x",
    "html-webpack-plugin": "3.2.x",
    "husky": "1.3.x",
    "mini-css-extract-plugin": "0.5.x",
    "node-sass": "4.11.x",
    "npm-run-all": "4.1.x",
    "sass-lint": "1.12.x",
    "sass-loader": "7.1.x",
    "source-map-loader": "0.2.x",
    "style-loader": "0.23.x",
    "terser-webpack-plugin": "1.2.x",
    "tslint": "5.12.x",
    "tslint-react": "3.6.x",
    "typescript": "3.2.x",
    "typescript-tslint-plugin": "0.2.x",
    "webpack": "4.28.x",
    "webpack-cli": "3.1.x",
    "webpack-dev-server": "3.1.x"
  },
  "peerDependencies": {
  }
